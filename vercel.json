{
  "version": 2,
  "name": "businesspro-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://businesspro-xelr.onrender.com"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://businesspro-xelr.onrender.com"
    }
  }
}
